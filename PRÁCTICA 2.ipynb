    "\n",
    "numero = 100\n",
    "\n",
    "# Calcula el logaritmo en base e\n",
    "log_e = log(numero)\n",
    "\n",
    "# Calcula el logaritmo en base 10\n",
    "log_10 = log(numero, base=10)\n",
    "\n",
    "# Calcula el cociente de los logaritmos\n",
    "cociente_log = log_e / log_10\n",
    "\n",
    "# Muestra los resultados\n",
    "log_e, log_10, cociente_log\n"
